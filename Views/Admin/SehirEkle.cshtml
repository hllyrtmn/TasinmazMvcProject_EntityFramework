@model TasinmazMvc.Models.IL
@{
    Layout = null;
}

@using (Html.BeginForm("SehirEkle", "Admin", FormMethod.Post))
{
    

    
    @Html.HiddenFor(model => model.ID)
    <div class="form-group">
        @Html.LabelFor(m => m.AD, new { @class = "control-label" })
        @Html.EditorFor(m => m.AD, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <input type="submit" value="Onayla" class="btn btn-primary" />
        <input type="reset" value="Reset" class="btn btn-danger" />
    </div>

}

